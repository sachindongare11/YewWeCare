<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.utilitybucket">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/yeswecare"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/yeswecare_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".Home" />
        <activity android:name=".RetrieveActivity" />
        <activity android:name=".Select" />
        <activity android:name=".homeimprovement"></activity>
    </application>

</manifest>